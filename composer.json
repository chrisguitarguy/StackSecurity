{
    "name": "chrisguitarguy/stack-security",
    "description": "Using the Symfony security component as Stack Middleware",
    "license": "MIT",
    "keywords": ["security", "middleware"],
    "require": {
        "php": ">=5.5",
        "symfony/http-foundation": "~2.6",
        "symfony/http-kernel": "~2.6",
        "symfony/security-core": "~2.6",
        "symfony/security-http": "~2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "stack/callable-http-kernel": "~1.0",
        "stack/builder": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Chrisguitarguy\\StackSecurity\\": "src/"
        }
    }
}
